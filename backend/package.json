{
  "name": "@mymcp/backend",
  "version": "1.0.0",
  "description": "MyMCP.me backend - MCP server and API for browser automation platform",
  "author": "MyMCP.me",
  "homepage": "https://mymcp.me",
  "bugs": "https://github.com/AlphaDataOmega/mymcp-me/issues",
  "type": "module",
  "bin": {
    "mcp-server-browsermcp": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsup src/index.ts src/api.ts --format esm && shx chmod +x dist/*.js",
    "start": "node dist/index.js --api-only",
    "start:mcp": "node dist/index.js --mcp-only",
    "start:all": "node dist/index.js",
    "dev": "tsup src/index.ts src/api.ts --format esm --watch",
    "prepare": "npm run build",
    "inspector": "CLIENT_PORT=9001 SERVER_PORT=9002 pnpx @modelcontextprotocol/inspector node dist/index.js --mcp-only"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.8.0",
    "@supabase/supabase-js": "^2.53.0",
    "commander": "^13.1.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "uuid": "^10.0.0",
    "ws": "^8.18.1",
    "zod": "^3.24.2",
    "zod-to-json-schema": "^3.24.3"
  },
  "devDependencies": {
    "@types/chrome": "^0.1.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.0",
    "shx": "^0.3.4",
    "tsup": "^8.4.0",
    "typescript": "^5.6.2"
  }
}
